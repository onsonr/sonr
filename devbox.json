{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.10.5/.schema/devbox.schema.json",
  "packages": [
    "ipfs@latest",
    "golangci-lint@latest",
    "go-task@latest",
    "go_1_21@latest"
  ],
  "env": {
    "GOPATH": "$HOME/go",
    "PATH": "$HOME/go/bin:$PATH"
  },
  "shell": {
    "scripts": {
      "proto": [
        "make proto-gen"
      ],
      "templ": [
        "templ generate"
      ],
      "ps": [
        "devbox services ls"
      ]
    }
  }
}
