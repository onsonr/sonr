package pages

import (
	"github.com/labstack/echo/v4"
	"github.com/onsonr/sonr/nebula/components/blocks"
	"github.com/onsonr/sonr/nebula/components/sections"
)

func Home(c echo.Context) error {
	return echoResponse(c, homeView())
}

templ homeView() {
	@blocks.LayoutNoBody("Sonr.ID", true) {
		@sections.HeaderMarketingNav()
		@sections.SectionHero()
		@sections.FooterMarketingNav()
	}
}
