{
  "cSpell.words": [
    "codespace",
    "IPFS",
    "multibase",
    "Roadmap",
    "tendermint",
    "webauthn"
  ],
  "gitlens.views.formats.commits.label": "${❰ tips ❱➤  }${message}",
  "gitlens.views.formats.commits.description": "${author, }${agoOrDate}${tips}",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.views.commitDetails.files.compact": false,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#65cbe9",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#37474F",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "!**/.git/hooks": true,
    ".dart_tool": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.packages": true,
    "**/.project": true,
    "**/.settings": true,
    "**/largeFolder": true,
    "**/pubspec.lock": true,
    "**pubspec.lock": true,
    "app.iml": true,
    "Contents.json": true,
    "**/node_modules": true
  },
  "svg.preview.background": "custom",
  "typescript.preferences.importModuleSpecifier": "non-relative"
}
